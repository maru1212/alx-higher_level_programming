#!/usr/bin/python3

def safe_print_list(my_list = [], x = 0):
    count = 0
    for i in range(0, x):
        try:
            print("{}".format(my_list[i]), end="")
            count += 1
        except IndexError:
            break
    print("")
    return count

mlist = [1, 2, 3, 5]

safe_print_list(mlist, 5)
